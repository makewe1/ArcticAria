{
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "scripts": {
    "format": "prettier --write \"**/*.{js,mjs,cjs,jsx,ts,tsx,json,jsonc,css,scss,less,md,html,yml,yaml,vue,gql,graphql,liquid}\"",
    "dev:all": "concurrently \"npm run dev:auth\" \"npm run dev:todo\" \"npm run dev:blog\"",
    "dev:todo": "npx wrangler dev --config ./todo/wrangler.jsonc",
    "dev:auth": "npx wrangler dev --config ./auth/wrangler.jsonc",
    "dev:blog": "npx wrangler dev --config ./blog/wrangler.jsonc"
  }
}
